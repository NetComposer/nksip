{lib_dirs, ["deps"]}.

{sub_dirs, ["nksip", "samples/nksip_loadtest", "samples/nksip_pbx"]}.

{erl_opts, [
    % native,
    debug_info, 
    fail_on_warning, 
    {parse_transform, lager_transform}
]}.

{cover_enabled, true}.
{cover_export_enabled, true}.

{deps, [
    {lager, "1.0.0", {git, "git://github.com/basho/lager", {tag, "1.0.0"}}},
    {ranch, ".*", {git, "git://github.com/extend/ranch.git", {tag, "0.6.1"}}}
]}.



